[user]
	name = blangguth 
	email = ben.langguth@gmail.com
[credential]
	helper = cache
[alias]
	graph = log --graph --color --pretty=format:"%C(yellow)%H%C(green)%d%C(reset)%n%x20%cd%n%x20%cn%x20(%ce)%n%x20%s%n"
	stat = status -sb
	last = log -1 --stat
	unstage = reset HEAD --
	cached = diff --cached
    cm = commit -v -m
    cma = commit -v -a -m
    co = checkout
    df = diff
    dfc = diff --cached
    br = branch
    lg = log -p
    rh = reset --hard
    r1 = reset HEAD^
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
